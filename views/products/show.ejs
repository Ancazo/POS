<!doctype html>

<html lang="en">

<head>

    <link rel="stylesheet" href="style.css">

    <title>Raccoon POS</title>

</head>

<body>

    <div class="container">
        <table>
            <tr>
                <th>Sales Order ID</th>
                <th>Customer ID</th>
                <th>Product ID</th>
                <th>Quantity</th>
                <th>Unit Price</th>
                <th>Total Price</th>
                <th>Created On</th>
                <th>Updated On</th>

            </tr>
            <tr>
                <th><%= sales.slug %></th>
                <th><%= sales.customerId %></th>
                <th><%= sales.productId %></th>
                <th><%= sales.productQuantity %></th>
                <th><%= sales.productPrice %></th>
                <th><%= sales.totalPrice %></th>
                <th><%= sales.createdAt.toDateString() %></th>
                <th><%= sales.updatedAt.toDateString() %></th>
            </tr>
        </table>
    </div>
</body>
</html>